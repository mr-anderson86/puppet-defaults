{
  "name": "puppet-defaults",
  "version": "1.0.0",
  "author": "mr-anderson86",
  "summary": "Installs default configuration on all nodes",
  "license": "Apache 2.0",
  "source": "https://github.com/mr-anderson86/puppet-defaults",
  "project_page": "https://github.com/mr-anderson86/puppet-defaults",
  "issues_url": "https://github.com/mr-anderson86/puppet-defaults/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
   ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_range": ">= 1.0.0"
    },
    {
      "name": "saz-ntp",
      "version_range": ">=2.5.1"
    },
    {
      "name": "saz-timezone",
      "version_range": ">=3.4.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.6.2 <= 6.7.2"
    }
  ]
}

